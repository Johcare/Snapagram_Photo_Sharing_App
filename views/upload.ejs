        <a href = "/photos" class = "page-link">Home</a>
         <a href = "/logout" class = "page-link">Logout</a>
    </div>
</div>


<body class = "text-center">
	<form class="upload" enctype="multipart/form-data" method="post">
		<% if (typeof error !== "undefined") { %>
			<p class="upload-error">
				<%- error %>
			</p>
		<% } %>
		<input type="file" name="file"/>
		<button class= "btn-group">Upload</button>
	</form>

</body>
</body>
